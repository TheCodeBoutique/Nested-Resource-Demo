<h1>Editing registration</h1>



<% form_for([@event, @registration]) do |f| -%>
  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Email</b><br />
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', [@event, @registration] %> |
<%= link_to 'Back', event_registrations_path(@event) %>
